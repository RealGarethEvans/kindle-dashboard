<g id="sun-times" font-size="{{SUN_FONT_SIZE}}" font-weight="bold">
    <svg  x="{{SUNRISE_ICON_X}}" y="{{SUN_ICON_Y}}" width="{{SUNRISE_ICON_WIDTH}}px" height="{{SUNRISE_ICON_HEIGHT}}px">
      <use xlink:href="#sunrise" x="0" y="0" transform="scale({{SUNRISE_ICON_SCALE}})"></use>
    </svg>
    <text style="text-anchor:middle;" x="{{SUNRISE_X}}" y="{{SUNRISE_Y}}">{{SUNRISE_TIME}}</text>
    <text style="text-anchor:middle;" x="{{SUNSET_X}}" y="{{SUNRISE_Y}}">{{SUNSET_TIME}}</text>
    <svg  x="{{SUNSET_ICON_X}}" y="{{SUN_ICON_Y}}" width="{{SUNRISE_ICON_WIDTH}}" height="{{SUNRISE_ICON_HEIGHT}}">
      <use xlink:href="#sunrise" x="0" y="0" transform="scale({{SUNRISE_ICON_SCALE}})"></use>
    </svg>
  </g>
